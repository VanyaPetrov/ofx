{
  "version": "1.1.0",
  "description": ".Net Core Library to work with OFX (Open Financial Exchange). More info http://www.ofx.org",
  "packOptions": {
    "summary": ".Net Core Library to work with OFX format. Ofx objects are generated from XSD version 1.0.3. Contains OFX and pure Sgml serializers.",
    "tags": [ "netcore", "ofx", "qfx", "money", "expense manager", "finance", "parser", "serializer", "sgml" ],
    "owners": [ "Mocoding" ],
    "releaseNotes": "Initial release. Included: OfxSerializer, SgmlSerializer, set of OFX objects generated from XSD. Unit tests",
    "projectUrl": "https://github.com/mocoding-software/ofx",
    "iconUrl": "https://mocoding.blob.core.windows.net/resources/ofx/nugetIcon.png",
    "licenseUrl": "https://raw.githubusercontent.com/mocoding-software/ofx/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/mocoding-software/ofx"
    }
  },

  "frameworks": {
    "netstandard1.0": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8+dnxcore50"
      ],
      "dependencies": {
        "System.Xml.XmlSerializer": "4.3.0",
        "System.Text.RegularExpressions": "4.3.0"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Xml": "4.0.0",
        "System.Xml.XmlSerializer": "4.0.0"
      }
    },
    "uap10.0": {
      "dependencies": {
        "System.Xml.XmlSerializer": "4.3.0",
        "System.Text.RegularExpressions": "4.3.0"
      }
    },
    "netcore45": {
      "dependencies": {
        "System.Runtime": "4.0.0",
        "System.Xml.XmlSerializer": "4.0.0",
        "System.Xml.ReaderWriter": "4.0.0",
        "System.Text.RegularExpressions": "4.0.0",
        "System.IO": "4.0.0"
      }
    }
  }
}

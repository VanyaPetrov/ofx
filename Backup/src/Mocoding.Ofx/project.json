{
  "version": "1.1.1",
  "description": ".Net Library to work with OFX (Open Financial Exchange). More info http://www.ofx.org",
  "packOptions": {
    "summary": ".Net Library to work with OFX format. Ofx objects are generated from XSD version 1.0.3. Contains OFX and pure Sgml serializers.",
    "tags": [ "netstandadrd", "ofx", "qfx", "money", "expense manager", "finance", "parser", "serializer", "sgml" ],
    "owners": [ "MOCODING, LLC" ],
    "releaseNotes": "Added support for NetStandard 1.0",
    "projectUrl": "https://github.com/mocoding-software/ofx",
    "iconUrl": "https://mocoding.blob.core.windows.net/resources/ofx/nugetIcon.png",
    "licenseUrl": "https://raw.githubusercontent.com/mocoding-software/ofx/master/LICENSE",
    "requireLicenseAcceptance": false,
    "repository": {
      "type": "git",
      "url": "https://github.com/mocoding-software/ofx"
    }
  },

  "buildOptions": {
    "additionalArguments": [ "/ruleset:../../_stylecop/StyleCop.ruleset" ]
  },

  "frameworks": {
    "netstandard1.0": { }
  },
  "dependencies": {
    "System.Xml.XmlSerializer": "4.3.0",
    "System.Text.RegularExpressions": "4.3.0",
    "StyleCop.Analyzers": "1.0.0"
  }
}
